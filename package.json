    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.4",
    "typescript": "~5.4.0",
    "typescript-eslint": "^8.23.0",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.1.4"
